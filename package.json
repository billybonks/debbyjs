  "devDependencies": {
    "eslint": "^4.18.2"
  },
  "directories": {
    "lib": "src"
  },
  "jest": {
    "projects": [
      {
        "displayName": "tests",
        "testRegex": ".*(test.js)",
        "testPathIgnorePatterns": [
          "/__snapshots__/",
          "shared/"
        ]
      },
      {
        "testPathIgnorePatterns": [
          "api",
          "coverage"
        ],
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ]
      }
    ]
  }
